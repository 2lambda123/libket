{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [    
        {
            "name": "Teleport",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/teleport",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {
                    "name": "KET_QUANTUM_EXECUTOR",
                    "value": "${workspaceFolder}/build/lib/kbw.kqe"
                },
                {
                    "name": "KBW_SEED",
                    "value": "1"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "Bell",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/bin/bell",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [
                {
                    "name": "KET_QUANTUM_EXECUTOR",
                    "value": "${workspaceFolder}/build/lib/kbw.kqe"
                },
                {
                    "name": "KBW_SEED",
                    "value": "113"
                }
            ],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}